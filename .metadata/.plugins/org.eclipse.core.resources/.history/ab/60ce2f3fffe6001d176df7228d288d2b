package LojaJogo;

public class Online_jogo extends LojaJogo {

	
		public Online_jogo(int catalojo) {
		super(catalojo);
		
	}
	
		 public String jogo;
			public float preços;
			public String controle;
			public String console;

		
		public void catalogo(int catelogo,String jogo,float preços,String controle,String console) {
		this.jogo =jogo;
		this.preços =preços;
		this.controle = controle;
		this.console = console;
			
		}
			
		
		
	public String getJogo() {
			return jogo;
		}

		public void setJogo(String jogo) {
			this.jogo = jogo;
		}

		public float getPreço() {
			return preços;
		}

		public void setPreço(float preços) {
			this.preços = preços;
		}

		public String getControle() {
			return controle;
		}

		public void setControle(String controle) {
			this.controle = controle;
		}

		public String getConsole() {
			return console;
		}

		public void setConsole(String console) {
			this.console = console;
		}
		
		@Override
		public void visualizar() {
			System.out.println("Qual o nome do jogo" + this.jogo);
			 System.out.println("O preço é: " + this.preços);
			 System.out.println("Quantidade de controles " + this.controle);
			 System.out.println("Modelo do Console" + this.console);

		}
}

	


